<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.gestaoentregas.FXController.ListaDePedidosController">
   <children>
      <TableView fx:id="tabelaSelecaoEntregasMotorista" prefHeight="400.0" prefWidth="300.0">
        <columns>
          <TableColumn fx:id="colunaID" prefWidth="86.0" text="ID" />
          <TableColumn fx:id="colunaLocal" prefWidth="213.0" text="LOCAL" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <VBox fx:id="painelDetalhes" layoutX="300.0" layoutY="25.0" prefHeight="375.0" prefWidth="300.0" style="-fx-background-color: white;">
         <children>
            <Label fx:id="lblDetalhesID" prefHeight="25.0" prefWidth="300.0" text="ID DA ENTREGA: ">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </VBox>
   </children>
</AnchorPane>
