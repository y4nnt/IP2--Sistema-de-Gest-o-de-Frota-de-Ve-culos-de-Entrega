sRenderer;\r\n\r\n    // Função ASYNC para usar o await nas bibliotecas\r\n    async function initMap() {\r\n        // 1. Importar bibliotecas necessárias\r\n        const { Map } = await google.maps.importLibrary(\"maps\");\r\n        const { DirectionsService, DirectionsRenderer } = await google.maps.importLibrary(\"routes\");\r\n        const { Autocomplete } = await google.maps.importLibrary(\"places\");\r\n\r\n        // 2. Inicializar Mapa\r\n        map = new Map(document.getElementById('map'), {\r\n            center: { lat: -8.0578381, lng: -34.8828969 },\r\n            zoom: 13,\r\n            mapId: \"DEMO_MAP_ID\" // Recomendado versão moderna\r\n        });\r\n\r\n        // 3. Configurar Serviços de Rota\r\n        directionsService = new DirectionsService();\r\n        directionsRenderer = new DirectionsRenderer();\r\n        directionsRenderer.setMap(map);\r\n\r\n        // 4. Configurar Autocomplete\r\n        const sourceInput = document.getElementById('source');\r\n        const destInput = document.getElementById('dest');\r\n\r\n        new Autocomplete(sourceInput);\r\n        new Autocomplete(destInput);\r\n    }\r\n\r\n    function calcRoute() {\r\n        const source = document.getElementById('source').value;\r\n        const dest = document.getElementById('dest').value;\r\n\r\n        if (!source || !dest) {\r\n            alert(\"Por favor, preencha origem e destino.\");\r\n            return;\r\n        }\r\n\r\n        let request = {\r\n            origin: source,\r\n            destination: dest,\r\n            travelMode: 'DRIVING'\r\n        };\r\n\r\n        directionsService.route(request)\r\n            .then((result) => {\r\n                directionsRenderer.setDirections(result);\r\n            })\r\n            .catch((e) => {\r\n                window.alert(\"Não foi possível traçar a rota: \" + e);\r\n            });\r\n    }\r\n\r\n    // Iniciar o mapa\r\n    initMap();\r\n</script>\r\n</body>\r\n</html>
